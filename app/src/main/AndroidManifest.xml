<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="embedded.kocaeli.edu.tr.embeddedmuzeuygulamasi">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/m1"
        android:name=".AppController"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity android:name=".YoutubeActivity"></activity>
        <activity
            android:name=".StartActivty"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DecoderActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".MuseumGeneralActivity"></activity>
        <activity android:name=".GeneralActivity"></activity>
        <activity android:name=".MuseumActivity"></activity>
        <activity android:name=".RelicListActivity"></activity>
    </application>

</manifest>
